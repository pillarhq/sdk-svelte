{
  "name": "@pillar-ai/svelte",
  "version": "0.1.8",
  "description": "Svelte bindings for Pillar Embedded Help SDK",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "help",
    "documentation",
    "support",
    "chat",
    "ai"
  ],
  "author": "Pillar Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pillarhq/sdk-svelte",
    "directory": "packages/sdk-svelte"
  },
  "homepage": "https://trypillar.com",
  "bugs": {
    "url": "https://github.com/pillarhq/sdk-svelte/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "svelte": ">=4.0.0"
  },
  "dependencies": {
    "@pillar-ai/sdk": "^0.1.8"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "svelte": "^5.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0"
  },
